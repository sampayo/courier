<h1>Lista de Clientes</h1>
<p id="notice">
	<%= notice %>
</p>
<table width="80%">
	<tr>
		<th>Email</th>
		<th>Nombre</th>
		<th>Apellido</th>
		<th>Fnacimiento</th>
		<th></th>
		<th></th>
	</tr>
	<% @personas.each do |admin| %>
	<tr align="Center">
		<td><%= admin.email %></td>
		<td><%= admin.nombre %></td>
		<td><%= admin.apellido %></td>
		<td><%= admin.fNacimiento %></td>
		<td><%= link_to 'Show', admin_path(admin) %></td>
		<td><%= link_to 'Edit', edit_persona_path(admin) %></td>
	</tr>
	<% end %>
</table>
<br />
