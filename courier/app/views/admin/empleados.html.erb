<h1><%= t('listaempleado') %></h1>
<p id="notice">
	<%= notice %>
</p>
<table width="80%">
	<tr>
		<th>Email</th>
		<th><%= t('nombrez') %></th>
		<th><%= t('apellido') %></th>
		<th><%= t('fechanac') %></th>
		<th><%= t('cargo') %></th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% @personas.each do |admin| %>
	<tr align="Center">
		<td><%= admin.email %></td>
		<td><%= admin.nombre %></td>
		<td><%= admin.apellido %></td>
		<td><%= admin.fNacimiento %></td>
		<td><%= cargo(admin.empleados_id) %></td>
		<td><%= link_to t('mostrar'), admin_path(admin) %></td>
		<td><%= link_to t('editar'), edit_admin_path(admin) %></td>
	</tr>
	<% end %>
</table>
<br />

<%= link_to t('nuevoempleado'), new_admin_path %>