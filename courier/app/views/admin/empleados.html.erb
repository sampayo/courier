<h1>Lista de Empleados</h1>
<p id="notice">
	<%= notice %>
</p>
<table width="80%">
	<tr>
		<th>Email</th>
		<th>Nombre</th>
		<th>Apellido</th>
		<th>Fnacimiento</th>
		<th>cargo</th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% @personas.each do |admin| %>
	<tr align="Center">
		<td><%= admin.email %></td>
		<td><%= admin.nombre %></td>
		<td><%= admin.apellido %></td>
		<td><%= admin.fNacimiento %></td>
		<td><%= cargo(admin.empleados_id) %></td>
		<td><%= link_to 'Show', admin_path(admin) %></td>
		<td><%= link_to 'Edit', edit_admin_path(admin) %></td>
	</tr>
	<% end %>
</table>
<br />

<%= link_to 'New Empleado', new_admin_path %>